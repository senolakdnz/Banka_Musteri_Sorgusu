                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">(SELECT H.MusteriID, H.AcilmaTarihi, S.BolgeID, S.SehirAd,
